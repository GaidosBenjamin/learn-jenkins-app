{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "471112858623.dkr.ecr.eu-central-1.amazonaws.com/my-jenkins-app:#APP_VERSION#",
            "portMappings": [{
                "name": "nginx-80-tip",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": ".25 vCPU",
    "executionRoleArn": "arn:aws:iam::471112858623:role/ecsTaskExecutionRole"
}